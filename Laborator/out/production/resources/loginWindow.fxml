<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label layoutX="154.0" layoutY="81.0" text="Sign in">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="154.0" layoutY="134.0" text="User:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldUser" layoutX="252.0" layoutY="135.0" />
      <Label layoutX="154.0" layoutY="192.0" text="Password:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldPassword" layoutX="252.0" layoutY="193.0" />
      <Button layoutX="252.0" layoutY="258.0" mnemonicParsing="false" onAction="#handleLoginButtonAction" text="Log in">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
